              <% for(var i=0; i<posts.length;i++){
              if(posts[i]['featured']===true) {
              %>
              <div style="background-image: url(img/tacotest.png)" class="featureSlider">
                <img class="featuredFlag" src="./img/FeaturedFlag2x.png">
                <h1 class="featuredTitle"><%=posts[i]['longTitle']%></h1>
