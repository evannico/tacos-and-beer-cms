          <div class="post-small-title">
            <%=posts[i]['smallTitle']%></div>
          <img class = "post-img" src="img/leos.png" alt="tacos and beer"/>
          <div class="post-title"><%=posts[i]['longTitle']%>
          </div>
          <div class="post-author">by <span class="pink"><%=posts[i]['author']%></span> <%=posts[i]['date']%></div>
